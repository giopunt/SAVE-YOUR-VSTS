{
  "name": "SAVE YOUR VSTS",
  "author": "Giovanni Puntil",
  "short_name": "SYVSTS",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "This extension will save your VSTS ass by changing the complete pull request button functionality",
  "homepage_url": "https://github.com/giopunt/SAVE-YOUR-VSTS",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.visualstudio.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}